{
  "name": "storeapp",
  "version": "0.0.1",
  "private": true,
  "author": "Aditia Prasetio",
  "scripts": {
    "android": "ENVFILE=.env react-native run-android --variant=devDebug",
    "androidReleaseDev": "cd android && ENVFILE=.env.development ./gradlew assembleDevRelease",
    "androidReleaseStaging": "cd android && ENVFILE=.env.staging ./gradlew assembleStagingRelease",
    "androidRelease": "cd android && ENVFILE=.env.production ./gradlew assembleProductionRelease",
    "androidBundleReleaseDev": "cd android && ENVFILE=.env.development ./gradlew bundleRelease",
    "androidBundleReleaseStaging": "cd android && ENVFILE=.env.staging ./gradlew bundleRelease",
    "androidBundleRelease": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.9.2",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "axios": "^0.19.2",
    "jsc-android": "^241213.1.0",
    "npm": "^6.14.5",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-config": "^1.2.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
